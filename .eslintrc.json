{
  "extends": [],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "globals": {},
  "rules": {
    "jsx-quotes": [
      "warn",
      "prefer-double"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}